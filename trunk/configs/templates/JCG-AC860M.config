CONFIG_VENDOR=Ralink                     # 设置设备供应商为 Ralink
CONFIG_PRODUCT=MT7621                    # 设置产品型号为 MT7621

CONFIG_FIRMWARE_PRODUCT_ID="JCG-AC860M"  # 设置固件产品ID，最多12个字符
CONFIG_32M_REBOOT_FIXUP=y                 # 启用 32MB 重启修复功能，用于防止由于内存问题导致的重启故障

CONFIG_FIRMWARE_INCLUDE_SFE=y             # 启用 SFE（Shortcut Forwarding Engine）功能，提高路由器的转发性能

CONFIG_LINUXDIR=linux-3.4.x               # 设置 Linux 内核目录，指定固件构建所用的内核版本


#CONFIG_FIRMWARE_TYPE_ROOTFS_IN_RAM=y     # 启用在 RAM 中构建根文件系统模式（已注释），减少固件大小，但会增加内存使用

#CONFIG_FIRMWARE_CPU_900MHZ=y              # 强制将 MT7621 CPU 时钟设置为 900MHz（已注释），用于提高处理性能

#CONFIG_FIRMWARE_CPU_SLEEP=y               # 启用 MT7621 CPU 睡眠模式（已注释），在空闲时降低功耗


CONFIG_FIRMWARE_WIFI_DRIVER=5.0.4.0       # 设置 WiFi 驱动版本为 5.0.4.0，以确保无线功能正常

CONFIG_FIRMWARE_KERNEL_CONFIG="kernel-3.4.x-5.0.config" # 指定内核配置文件，用于配置内核特性


CONFIG_FIRMWARE_ENABLE_IPV6=y              # 启用 IPv6 支持，以便设备能够在 IPv6 网络中运行

CONFIG_FIRMWARE_ENABLE_USB=y               # 启用 USB 支持，允许连接 USB 设备如存储或打印机

CONFIG_FIRMWARE_ENABLE_UFSD=y              # 启用 NTFS 文件系统支持（通过 Paragon 驱动），以便访问 NTFS 格式的驱动器

CONFIG_FIRMWARE_INCLUDE_ANTFS=y            # 启用 AVM NTFS 驱动支持，提供对 NTFS 文件系统的额外支持

CONFIG_FIRMWARE_ENABLE_FAT=y               # 启用 FAT/FAT32 文件系统支持，允许访问 FAT 格式的存储设备

CONFIG_FIRMWARE_ENABLE_EXFAT=y             # 启用 exFAT 文件系统支持，适用于较大的存储设备和文件

CONFIG_FIRMWARE_ENABLE_EXT2=y              # 启用 EXT2 文件系统支持，Linux 系统中的常用文件系统

CONFIG_FIRMWARE_ENABLE_EXT3=y              # 启用 EXT3 文件系统支持，扩展的 Linux 文件系统，提供日志功能

CONFIG_FIRMWARE_ENABLE_EXT4=y              # 启用 EXT4 文件系统支持，现代 Linux 系统中最常用的文件系统

CONFIG_FIRMWARE_ENABLE_XFS=n               # 禁用 XFS 文件系统支持，XFS 是高性能的文件系统，但在此环境中不需要

CONFIG_FIRMWARE_ENABLE_FUSE=y              # 启用 FUSE（用户空间文件系统）支持，允许用户空间程序创建文件系统

CONFIG_FIRMWARE_ENABLE_SWAP=n               # 禁用交换文件/分区支持，减少内存使用

CONFIG_FIRMWARE_INCLUDE_UVC=y              # 包含 UVC 摄像头模块，支持 USB 视频设备

CONFIG_FIRMWARE_INCLUDE_HID=n               # 禁用 USB-HID 模块，表示不支持人机接口设备

CONFIG_FIRMWARE_INCLUDE_SERIAL=y            # 包含 USB-串口模块，允许通过 USB 连接串口设备

CONFIG_FIRMWARE_INCLUDE_AUDIO=y             # 包含 USB 音频模块，支持 USB 音频设备

CONFIG_FIRMWARE_INCLUDE_XFRM=n              # 禁用 XFRM（IPsec）模块和 iptables 扩展，不启用 IPsec 支持

CONFIG_FIRMWARE_INCLUDE_QOS=n               # 禁用网络 QoS 调度模块，不启用服务质量调度

CONFIG_FIRMWARE_INCLUDE_IMQ=n               # 禁用 IMQ 模块，不启用 IMQ 功能

CONFIG_FIRMWARE_INCLUDE_IFB=n               # 禁用 IFB 模块，不启用 IFB 功能

CONFIG_FIRMWARE_INCLUDE_IPSET=y             # 包含 IPSet 实用程序和内核模块，用于管理 IP 集合

CONFIG_FIRMWARE_INCLUDE_NFSD=n              # 禁用 NFSv3 服务器，不启用网络文件系统支持

CONFIG_FIRMWARE_INCLUDE_NFSC=n              # 禁用 NFSv3 客户端，不启用 NFS 网络文件访问

CONFIG_FIRMWARE_INCLUDE_CIFS=n              # 禁用 CIFS（SMB）客户端，不启用 CIFS 网络文件共享

CONFIG_FIRMWARE_INCLUDE_LANG_CN=y           # 包含 WebUI 国际化资源（简体中文），提供中文用户界面

# 其他语言的国际化资源（已注释）
