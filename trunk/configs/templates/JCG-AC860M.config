CONFIG_FIRMWARE_INCLUDE_NTFS_3G=n             # 禁用 NTFS-3G FUSE 驱动，不启用 NTFS-3G 支持

CONFIG_FIRMWARE_INCLUDE_LPRD=y                # 包含 LPR 打印机守护进程，支持打印机共享

CONFIG_FIRMWARE_INCLUDE_U2EC=y                # 包含 USB 以太网打印机守护进程，允许 USB 打印机通过网络共享

CONFIG_FIRMWARE_INCLUDE_TCPDUMP=y             # 包含 tcpdump 实用程序，用于网络流量监控和分析

CONFIG_FIRMWARE_INCLUDE_HDPARM=y              # 包含 hdparm 实用程序，用于配置硬盘参数和管理电源

CONFIG_FIRMWARE_INCLUDE_PARTED=y              # 包含 parted 实用程序，用于磁盘分区管理

CONFIG_FIRMWARE_INCLUDE_SMBD=y                # 包含 SMB 服务器，支持通过 SMB 共享文件

CONFIG_FIRMWARE_INCLUDE_SMBD36=y              # 使用 samba 3.6 而不是 samba 3，以确保兼容性和功能性

CONFIG_FIRMWARE_INCLUDE_WINS=n                # 禁用 WINS 服务器，不启用 WINS 名称解析服务

CONFIG_FIRMWARE_INCLUDE_SMBD_SYSLOG=n         # 禁用 SMB 和 WINS 服务器的 syslog，不记录日志信息

CONFIG_FIRMWARE_INCLUDE_FTPD=y                # 包含 FTP 服务器，支持文件传输服务

CONFIG_FIRMWARE_INCLUDE_RPL2TP=n               # 禁用 RP-L2TP 客户端，不启用 L2TP 功能

CONFIG_FIRMWARE_INCLUDE_EAP_PEAP=y            # 包含 EAP-TTLS 和 EAP-PEAP 认证支持，适用于无线网络安全认证

CONFIG_FIRMWARE_INCLUDE_HTTPS=n               # 禁用 HTTPS 支持，不启用加密的 HTTP 服务

CONFIG_FIRMWARE_INCLUDE_SFTP=y                # 包含 sftp-server，支持安全的文件传输协议

CONFIG_FIRMWARE_INCLUDE_DROPBEAR=n            # 禁用 dropbear SSH，使用其他 SSH 实现

CONFIG_FIRMWARE_INCLUDE_DROPBEAR_FAST_CODE=n  # 禁用 dropbear 对称密码和哈希加速，使用默认加密设置

CONFIG_FIRMWARE_INCLUDE_OPENSSH=y             # 包含 OpenSSH 代替 dropbear，提供更全面的 SSH 功能

CONFIG_FIRMWARE_INCLUDE_OPENVPN=y             # 包含 OpenVPN，支持 VPN 连接

CONFIG_FIRMWARE_INCLUDE_SSWAN=n                # 禁用 StrongSwan，未启用 IPsec VPN 支持

CONFIG_FIRMWARE_INCLUDE_OPENSSL_EC=y          # 包含椭圆曲线（EC）到 OpenSSL 库，支持更强的加密算法

CONFIG_FIRMWARE_INCLUDE_OPENSSL_EXE=y         # 包含用于生成证书的 OpenSSL 可执行文件，支持 SSL/TLS 功能

CONFIG_FIRMWARE_INCLUDE_XUPNPD=y              # 包含 xUPNPd IPTV 媒体服务器，支持 IPTV 服务

CONFIG_FIRMWARE_INCLUDE_MINIDLNA=n            # 禁用 Minidlna UPnP 媒体服务器，不支持媒体流服务

CONFIG_FIRMWARE_INCLUDE_FIREFLY=n             # 禁用 Firefly iTunes 媒体服务器，不支持 iTunes 媒体流

CONFIG_FIRMWARE_INCLUDE_FFMPEG_NEW=y           # 包含 ffmpeg 0.11.x 版本，支持音视频处理和转码

CONFIG_FIRMWARE_INCLUDE_TRANSMISSION=n        # 禁用 Transmission torrent，未启用种子下载功能

CONFIG_FIRMWARE_INCLUDE_TRANSMISSION_WEB_CONTROL=n # 禁用 Transmission-Web-Control，未启用种子管理的 Web 界面

CONFIG_FIRMWARE_INCLUDE_ARIA=y                 # 包含 Aria2 下载管理器，支持多种下载协议

CONFIG_FIRMWARE_INCLUDE_ARIA_WEB_CONTROL=y     # 包含 Aria2 WEB 控制，支持通过 Web 界面管理下载

CONFIG_FIRMWARE_INCLUDE_CURL=y                 # 包含 CURL 库，支持网络请求和数据传输

CONFIG_FIRMWARE_INCLUDE_SCUTCLIENT=n           # 禁用 SCUT 客户端，未启用校园网认证功能

CONFIG_FIRMWARE_INCLUDE_GDUT_DRCOM=n           # 禁用 GDUT DRCOM 客户端，未启用校园网认证功能

CONFIG_FIRMWARE_INCLUDE_DOGCOM=y                # 包含 DOGCOM 客户端，支持某些网络的认证

CONFIG_FIRMWARE_INCLUDE_MINIEAP=y              # 包含 MINIEAP 客户端，支持 EAP 认证功能

CONFIG_FIRMWARE_INCLUDE_NJIT_CLIENT=y          # 包含 NJIT 客户端，支持新泽西理工学院网络认证

CONFIG_FIRMWARE_INCLUDE_NAPT66=y                # 包含 NAPT66 支持，允许多个内部 IPv6 地址共享公共 IPv6 地址

CONFIG_FIRMWARE_INCLUDE_SOFTETHERVPN_SERVER=n  # 禁用 SoftEther VPN 服务器，未启用 SoftEther VPN 功能

CONFIG_FIRMWARE_INCLUDE_SOFTETHERVPN_CLIENT=n  # 禁用 SoftEther VPN 客户端，未启用 SoftEther VPN 功能

CONFIG_FIRMWARE_INCLUDE_SOFTETHERVPN_CMD=n     # 禁用 SoftEther VPN 命令，未启用 SoftEther VPN 功能

CONFIG_FIRMWARE_INCLUDE_VLMCSD=y               # 包含 VLMCSD，支持视频流处理和管理

CONFIG_FIRMWARE_INCLUDE_TTYD=y                 # 包含 TTYD，支持通过浏览器访问命令行

CONFIG_FIRMWARE_INCLUDE_LRZSZ=y                # 包含 LRZSZ，用于串行终端间的文件传输

CONFIG_FIRMWARE_INCLUDE_HTOP=y                 # 包含 HTOP，交互式系统监控工具

CONFIG_FIRMWARE_INCLUDE_NANO=y                 # 包含 NANO，简单易用的文本编辑器

CONFIG_FIRMWARE_INCLUDE_IPERF3=y               # 包含 iperf3，用于网络性能测试

CONFIG_FIRMWARE_INCLUDE_DUMP1090=n             # 禁用 dump1090，未启用 ADS-B 信号接收

CONFIG_FIRMWARE_INCLUDE_RTL_SDR=n              # 禁用 RTL-SDR，未启用软件定义无线电功能

CONFIG_FIRMWARE_INCLUDE_MTR=y                  # 包含 MTR，网络诊断工具

CONFIG_FIRMWARE_INCLUDE_SOCAT=y                # 包含 socat，多功能数据流转发工具

CONFIG_FIRMWARE_INCLUDE_SRELAY=y               # 包含 Srelay，SOCKS 代理服务器

CONFIG_FIRMWARE_INCLUDE_MENTOHUST=n            # 禁用 Mentohust，未启用校园网认证功能

CONFIG_FIRMWARE_INCLUDE_FRPC=n                 # 禁用 FRPC，未启用内网穿透功能

CONFIG_FIRMWARE_INCLUDE_FRPS=n                 # 禁用 FRPS，未启用内网穿透功能

CONFIG_FIRMWARE_INCLUDE_TUNSAFE=n               # 禁用 TUNSAFE，未启用相关功能

CONFIG_FIRMWARE_INCLUDE_WIREGUARD=n            # 禁用 WireGuard，未启用 VPN 功能

CONFIG_FIRMWARE_INCLUDE_MSD_LITE=y             # 包含 MSD Lite，支持 MSD 功能
